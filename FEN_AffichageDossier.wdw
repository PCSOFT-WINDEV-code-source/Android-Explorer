   -
     name : OPT_Supprimer
     identifier : 0x281b3ba40508993f
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1-
          // Confirmation utilisateurs
          si Dialogue("Voulez-vous supprimer les éléments sélectionnées ?", ["Supprimer","Annuler"], 1, 2, dlgIcôneQuestion) = 1
          	
          	// On supprime tout ce qui est sélectionner
          	pour TOUTE LIGNE SELECTIONNEE DE ZR_DossiersFichiers
          		// Suppression de chaque élément
          		SuppressionElement(ZR_DossiersFichiers.ATT_CheminComplet,ZR_DossiersFichiers.ATT_Type)
          	FIN
          	
          	// Suppression double RC
          	gsListeAAfficher = remplace(gsListeAAfficher,rc+rc,rc)
          	
          	// Affiche un message à l'utilisateur
          	ToastAffiche("Suppression terminée")
          	
          	// Rafraîchit la fenêtre	
          	InitialisationFenêtre()
          fin
         type : 32
   -
     name : OPT_Coller
     identifier : 0x283814f104d9c8d5
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         code : |1-
          // On désactive l'option
          OPT_Coller..Visible = faux
          
          // Selon l'option sélectionné on fait ce qu'il faut
          si gnModeCopierCouper = copier ALORS
          	si RéaliseCopier = faux ALORS
          		retour
          	FIN
          FIN
          
          SI gnModeCopierCouper = couper ALORS
          	// Un couper est en faite un copier coller puis un supprimer
          	
          	// On copie 
          	si RéaliseCopier() = faux ALORS
          		retour
          	FIN
          	
          	// On supprime
          	POUR TOUT CHAÎNE sFichier de gsFichierCopierCouper
          		
          		// Suppression des éléments
          		SuppressionElement(sFichier)
          	FIN
          FIN
          
          // RAZ de la fenêtre
          InitialisationFenêtre(gsFichierCopierCouper)
          
          // RAZ des infos du mode copier coller
          gsFichierCopierCouper = ""
          gnModeCopierCouper = AucuneOpération
          
          
          	procedure interne RéaliseCopier
          	// On parcourt tous les fichiers pour réaliser la copie
          	POUR TOUT CHAÎNE sFichier de gsFichierCopierCouper
          		
          		// Cas d'erreur
          		SI sFichier ~="" ALORS
          			CONTINUER
          		FIN
          		
          		// Copie des répertoires
          		SI fRepExiste(sFichier) ALORS
          			// On crée le répertoire
          			fRepCrée(gsRepEnCours+[fSep]+ExtraitChaîne(fExtraitChemin(sFichier,fRépertoire),1,fSep,DepuisFin))
          			
          			// On fait la copie à la main
          			si fCopieFichier(sFichier+[fSep]+"*.*",gsRepEnCours+[fSep]+sFichier,frRécursif) = faux ALORS
          				erreur(ErreurInfo(errComplet))
          				renvoyer faux
          			FIN
          		FIN
          		
          		// Copie de fichiers
          		SI fFichierExiste(sFichier) ALORS
          			si fCopieFichier(sFichier,gsRepEnCours+[fSep]) = faux ALORS
          				Erreur(ErreurInfo(errComplet))
          				renvoyer faux
          			FIN
          		FIN
          	FIN
          	renvoyer vrai
          	FIN
         type : 32

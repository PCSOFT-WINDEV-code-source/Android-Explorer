  procedures :
   -
     name : GestionSélectionDossier
     procedure_id : 1737049898816999268
     type_code : 14
     code : |1+
      // Résumé : Gère la sélection ou le lancement d'une application associée
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE GestionSélectionDossier()
      
      // cas d'erreur
      si ZR_DossiersFichiers <= 0 ou ZR_DossiersFichiers > ZR_DossiersFichiers..Occurrence ALORS
      	retour
      FIN
      
      // En mode sélection on ajoute un élément un la sélection
      si gbModeSélection = vrai ALORS
      	// On colorise l’élément sélectionné
      	ZR_DossiersFichiers[ZR_DossiersFichiers].ATT_Selection = PAS(ZR_DossiersFichiers[ZR_DossiersFichiers].ATT_Selection)
      	SI ZR_DossiersFichiers[ZR_DossiersFichiers].ATT_Selection = faux ALORS
      		// Suppression de la sélection de la sélection
      		ZR_DossiersFichiers[ZR_DossiersFichiers]..CouleurFond = Transparent
      		
      		// si plus de sélection
      		gbModeSélection = VérifieSelection()
      		
      	SINON
      		// On sélectionne l'élément en plus
      		ZR_DossiersFichiers[ZR_DossiersFichiers]..CouleurFond = GrisFoncé
      	FIN
      sinon
      	
      	// On ouvre à nouveau la fenêtre avec paramètres différentes (permet d'avoir le retour d'activité via le bouton retour sans avoir à coder quoi que ce soit)
      	gsFichierListe = ""
      	ListerType(geTypeExtensionEnCours,Faux,ZR_DossiersFichiers[ZR_DossiersFichiers].ATT_CheminComplet)
      	
      	tantque gsFichierListe = ""
      		multitache(-10)
      	FIN
      	
      	// Vide 
      	SI gsFichierListe = "__-1__" ALORS
      		info(<§$0001§>)
      		RETOUR
      	FIN
      		
      	OuvreFenêtreMobile(FEN_AffichageDossier,gsFichierListe,ZR_DossiersFichiers[ZR_DossiersFichiers].LIB_Nom,geTypeExtensionEnCours,Faux,ZR_DossiersFichiers[ZR_DossiersFichiers].ATT_CheminComplet,"GestionOptionDispo")
      		
      FIN
     type : 458752
   -
     name : AfficheContenu
     procedure_id : 1737054258210082430
     type_code : 14
     code : |1-
      // Résumé : Affiche le contenu de la variable gsListeAAfficher dans la fenêtre
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AfficheContenu()
      
      // On vide la zone avant d'afficher le contenu 
      ZoneRépétéeSupprimeTout(ZR_DossiersFichiers)
      
      si gbModeRep ALORS
      	////////////////////////
      	// MODE REPERTOIRE
      	///////////////////////
      	
      	
      	// Tableau contenant tous les fichiers / dossiers avec les informations pour leur affichages
      	taDossier est un tableau associatif d'entiers
      	
      	// On récupère les dossiers contenant les fichiers
      	POUR TOUTE CHAÎNE sFichier DE gsListeAAfficher SEPAREE PAR RC
      		si taDossier[fExtraitChemin(sFichier,fDisque+fRépertoire)] = 0 ALORS
      			// Nouveau dossier
      			taDossier[fExtraitChemin(sFichier,fDisque+fRépertoire)] = 1
      		sinon
      			// Dossier déjà existant, on ajoute le fait qu'un fichier est aussi dans ce dossier
      			taDossier[fExtraitChemin(sFichier,fDisque+fRépertoire)]+=1
      		FIN
      	FIN
      	
      	// On ajoute les dossiers
      	pour tout nValeur,sClé de taDossier
      	
      		// Cas d'erreur
      		si sClé ~= "" ALORS
      			continuer
      		FIN
      		
      		// Vérification de la recherche
      		SI ACTB_ActionBar..ValeurRecherche <> "" _ET_ Contient(sClé,ACTB_ActionBar..ValeurRecherche,SansCasse) = Faux ALORS
      			// passe à l'élément suivant
      			CONTINUER
      		FIN	
      		
      		// On récupère le nom du dossier contenant les fichiers
      		nIndiceAjout est un entier = ZoneRépétéeAjouteLigne(ZR_DossiersFichiers)
      		
      		si sClé+[fsep] = SysRepCarteStockage()+[fsep] ALORS
      			ZR_DossiersFichiers[nIndiceAjout].LIB_Nom = "Mémoire de stockage interne"
      		sinon
      			ZR_DossiersFichiers[nIndiceAjout].LIB_Nom = ExtraitChaîne(sClé,2,fSep,DepuisFin) + " (" + nValeur + ")"
      		FIN
      		
      		// On remplit les infos
      		ZR_DossiersFichiers[nIndiceAjout].ATT_CheminComplet = sClé
      		ZR_DossiersFichiers[nIndiceAjout].ATT_Selection = faux
      		ZR_DossiersFichiers[nIndiceAjout].ATT_Type = type_Dossier
      	FIN
      	
      sinon
      	
      	////////////////////////
      	// MODE FICHIER + REPERTOIRE
      	///////////////////////
      	
      	POUR TOUTE CHAÎNE sFichier DE gsListeAAfficher SEPAREE PAR RC
      		
      		// Cas d'erreur
      		SI sFichier ~= "" ALORS
      			CONTINUER
      		FIN
      		
      		// Vérification de la recherche
      		SI ACTB_ActionBar..ValeurRecherche <> "" _ET_ Contient(sFichier,ACTB_ActionBar..ValeurRecherche,SansCasse) = Faux ALORS
      			// passe à l'élément suivant
      			CONTINUER
      		FIN	
      		
      		// Indice d'ajout
      		nIndiceAjout est un entier
      		
      		// Dossier ou fichier ?
      		si fRepExiste(sFichier) = vrai ALORS
      			// Dossier
      			nIndiceAjout = ZoneRépétéeAjouteLigne(ZR_DossiersFichiers)
      			
      			
      			// cas particulier de la mémoire interne
      			si sFichier = SysRepCarteStockage ALORS
      				ZR_DossiersFichiers[nIndiceAjout].LIB_Nom = "Mémoire de stockage interne"
      			sinon
      				ZR_DossiersFichiers[nIndiceAjout].LIB_Nom = ExtraitChaîne(sFichier,1,fSep,DepuisFin)
      			FIN
      			
      			// On remplit les infos
      			ZR_DossiersFichiers[nIndiceAjout].ATT_CheminComplet = sFichier
      			ZR_DossiersFichiers[nIndiceAjout].ATT_Selection = Faux
      			ZR_DossiersFichiers[nIndiceAjout].ATT_Type = type_Dossier
      		sinon
      			
      			si fFichierExiste(sFichier) = vrai
      				// Fichier
      				nIndiceAjout = ZoneRépétéeAjouteLigne(ZR_DossiersFichiers)
      				
      				// On remplit les infos
      				ZR_DossiersFichiers[nIndiceAjout].LIB_Nom = ExtraitChaîne(sFichier,1,fSep,DepuisFin)
      				ZR_DossiersFichiers[nIndiceAjout].ATT_CheminComplet = sFichier
      				ZR_DossiersFichiers[nIndiceAjout].ATT_Type = type_Fichier
      				ZR_DossiersFichiers[nIndiceAjout].IMG_Logo = getImageSelonExtension(sFichier)
      				ZR_DossiersFichiers[nIndiceAjout].ATT_Selection = Faux
      			fin
      		FIN
      		
      	FIN
      FIN
      
      // Gère les options affichés dans l'action bar
      GestionOptionDispo()
     type : 458752
   -
     name : RécupèreSélection
     procedure_id : 1737087338048426493
     type_code : 14
     code : |1+
      // Résumé : Récupère les éléments sélectionnés dans la fenêtre
      // Paramètres :
      // Valeur de retour :
      // 	chaîne UNICODE : liste des éléments sélectionnés
      //
      PROCEDURE RécupèreSélection() : chaine
      
      // On récupère la sélection 
      sChaineSélection est une chaine = ""
      
      // Pour tous les éléments possiblement sélectionnés (la ZR)
      POUR TOUTE LIGNE nIndice DE ZR_DossiersFichiers
      	
      	// Si l'élément est sélectionné on ajoute son chemin complet à la liste
      	si ZR_DossiersFichiers[nIndice].ATT_Selection = Vrai ALORS		
      		sChaineSélection+= [rc] + ZR_DossiersFichiers[nIndice].ATT_CheminComplet
      	FIN
      FIN
      
      // Renvoie la sélection
      renvoyer sChaineSélection
      
     type : 458752
   -
     name : GestionSélectionFichier
     procedure_id : 1742568115749095502
     type_code : 14
     code : |1+
      // Résumé : Gère la sélection ou le lancement d'une application associée
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE GestionSélectionFichier()
      
      // cas d'erreur
      SI ZR_DossiersFichiers <= 0 OU ZR_DossiersFichiers > ZR_DossiersFichiers..Occurrence ALORS
      	RETOUR
      FIN
      
      // Si on est en mode sélection, on ajoute un élément un la sélection
      si gbModeSélection = vrai ALORS
      	// On colorise l’élément sélectionné
      	ZR_DossiersFichiers[ZR_DossiersFichiers].ATT_Selection = pas(ZR_DossiersFichiers[ZR_DossiersFichiers].ATT_Selection)
      
      	// Si l'élément est déjà sélectionné, alors son attribut vient de passer à faux	
      	SI ZR_DossiersFichiers[ZR_DossiersFichiers].ATT_Selection = faux ALORS
      		ZR_DossiersFichiers[ZR_DossiersFichiers]..CouleurFond = Transparent
      		
      		// si plus de sélection
      		gbModeSélection = VérifieSelection()
      		
      	SINON
      		// L'élément vient d'être sélectionné, changement de la couleur de fond
      		ZR_DossiersFichiers[ZR_DossiersFichiers]..CouleurFond = GrisFoncé
      	FIN
      sinon
      	// Sinon on lance l'application associés au fichier
      	LanceAppliAssociée(ZR_DossiersFichiers.ATT_CheminComplet)
      FIN
     type : 458752
   -
     name : VérifieSelection
     procedure_id : 1742663790441951214
     type_code : 14
     code : |1+
      // Résumé : Vérifie la sélection courante
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	booléen : Vrai si une sélection est encore en cours
      //
      PROCEDURE VérifieSelection() : booléen
      
      // Par défaut on a pas de sélection
      bSelection est un booléen = Faux
      
      // parcourt la ZR
      POUR i=1 _a_ ZR_DossiersFichiers..Occurrence
      	
      	// Une sélection ? 
      	SI bSelection = Vrai ALORS renvoyer bSelection
      	
      	// Récupère l'attribut de la ZR
      	bSelection = ZR_DossiersFichiers[i].ATT_Selection
      FIN
      
      // Sélection ? (utile pour la dernière ligne
      SI bSelection = Vrai ALORS RENVOYER bSelection
      
      // pas de sélection
      renvoyer faux
     type : 458752
   -
     name : InitialisationFenêtre
     procedure_id : 1743775890618084684
     type_code : 14
     code : |1+
      // Résumé : Initialise la fenêtre 
      // Paramètres :
      //	sNouveauFichier (chaîne UNICODE - valeur par défaut="") :Nouveau fichier à ajouter si besoin
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE InitialisationFenêtre(local sNouveauFichier est une chaine = "")
      
      // Récupère les éléments possiblement ajouté par les fenêtre précédente
      gsListeAAfficher+=[rc]+sNouveauFichier
      
      // Affiche la ZR
      ZR_DossiersFichiers..Visible = Vrai
      
      // Affiche contenu
      AfficheContenu()
      
      // Gestion visibilité de l'option nouveau dossier
      OPT_Nouveau_dossier..Visible = PAS gbModeRep
      
      // On gère la sélection et les options disponible
      gbModeSélection = faux
      GestionOptionDispo
      
      // Supprime sélection globale
      ZoneRépétéeSelectMoins(ZR_DossiersFichiers)
      
     type : 458752
   -
     name : GestionOptionDispo
     procedure_id : 1744856135025048903
     type_code : 14
     code : |1+
      // Résumé : Gère les options disponibles selon les éléments sélectionnés
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE GestionOptionDispo()
      
      // récupère la sélection
      bEtatSelection est un booléen = VérifieSelection()
      
      // Met à jour la visibilité des options
      OPT_Copier..Visible = bEtatSelection
      OPT_Couper..Visible = bEtatSelection
      OPT_Supprimer..Visible = bEtatSelection
      OPT_Zip..Visible = bEtatSelection
      
      
      // On ne peut renommer qu'un élément, on ajuste la visibilité de l'option en fonction de la sélection courante 
      sSelectionCourante est une chaine = RécupèreSélection()
      si sSelectionCourante = "" ou contient(sSelectionCourante,rc) ALORS
      	OPT_Renommer..Visible = faux
      sinon	
      	OPT_Renommer..Visible = vrai
      FIN
      
      // gestion du mode copier, couper, coller
      si gnModeCopierCouper = AucuneOpération ALORS
      	OPT_Coller..Visible= faux
      sinon
      	OPT_Coller..Visible= Vrai
      FIN
      
     type : 458752
   -
     name : AnnuleSélection
     procedure_id : 1745166639685263747
     type_code : 14
     code : |1+
      // Résumé : Annule la sélection courante
      // Paramètres :
      //	Aucun
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE AnnuleSélection()
      
      
      // Annule la sélection
      pour TOUTE LIGNE i DE ZR_DossiersFichiers
      	ZR_DossiersFichiers[ZR_DossiersFichiers]..CouleurFond = Transparent
      	ZR_DossiersFichiers[ZR_DossiersFichiers].ATT_Selection = faux
      FIN
      
      // RAZ des infos
      gbModeSélection = Faux
      GestionOptionDispo()
      
     type : 458752
   -
     name : SuppressionElement
     procedure_id : 1754083794950157747
     type_code : 14
     code : |1+
      // Résumé : Supprime un élément
      // Paramètres :
      //	sCheminComplet (chaîne UNICODE) : Chemin de l'élément
      //	sType (chaîne UNICODE - valeur par défaut="") : Type de fichier
      // Valeur de retour :
      // 	Aucune
      //
      PROCEDURE SuppressionElement(local sCheminComplet est une chaine, local sType est une chaine = "")
      
      
      // Si type inconnue, alors on regarde si c'est un dossier ou un fichier
      si fRepExiste(sCheminComplet) ALORS
      	sType = type_Dossier
      FIN
      si fFichierExiste(sCheminComplet) ALORS
      	sType = type_Fichier
      FIN
      
      // Selon le type de l'élément courant
      SELON sType
      
      	// Dossier
      	CAS type_Dossier
      		
      		// Supprime le dossier en récursif
      		SI fRepSupprime(sCheminComplet,frRécursif)	 = Faux ALORS
      			Erreur(ErreurInfo(errComplet))
      			RETOUR
      		FIN
      		
      		// Supprime l'élément de la liste à afficher
      		gsListeAAfficher = 	Remplace(gsListeAAfficher,sCheminComplet,"")
      		
      	// Fichier				
      	CAS type_Fichier
      		
      		// Supprime le fichier
      		SI fSupprime(sCheminComplet) = Faux ALORS
      			Erreur(ErreurInfo(errComplet))
      			RETOUR
      		FIN
      		
      		// Supprime l'élément de la liste à afficher
      		gsListeAAfficher = 	Remplace(gsListeAAfficher,sCheminComplet,"")	
      		
      		
      	AUTRE CAS
      		// Erreur
      		Erreur("Type inconnue")
      FIN
     type : 458752

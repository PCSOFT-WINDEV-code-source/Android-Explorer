   -
     name : OPT_Zip
     identifier : 0x281b3c7f055aef30
     internal_properties : 
     code_elements :
      type_code : 26
      p_codes :
       -
         internal_properties : CAAAAAgAAACbXULZJfffll9sD2q8R6j5m60cbOhNAxM36nu4+NXPJ8HkVuDrCNl9+EMDBJCe9e5/CjpwEHTeEJKZHPGshHLQua1Qp8EgtJS4+lZAovbM1lfVpI/WD6ITRCSxCMM1i3ARMRONTjiuAvTGZzw=
         code : |1+
          // Préparation du zip
          sNomArchive est une chaine
          sNomArchiveComplet est une chaine
          
          // Saisie du nom de l'archive
          si saisie("Nom de l'archive ?",sNomArchive) = 0 _ou_ sNomArchive = "" ALORS
          	// On ne veut pas créer d'archive
          	retour
          FIN
          
          // On crée l'archive
          
          // Formate le nom de l'archive
          sNomArchiveComplet = sNomArchive
          sNomArchive=fExtraitChemin(sNomArchive,fFichier)
          
          // Si pas d'extension, on force le zip
          si fExtraitChemin(gsRepEnCours+[fSep]+sNomArchiveComplet,fExtension) ~= "" ALORS
          	sNomArchiveComplet +=".zip"
          FIN
          
          // Crée l'archive
          si zipCrée(sNomArchive,gsRepEnCours+[fsep]+sNomArchiveComplet) > 0  ALORS
          	erreur(erreurinfo(errComplet))
          	retour
          FIN
          
          // On ajoute les éléments sélectionnés dans l'archive
          pour TOUTE ligne i DE ZR_DossiersFichiers
          	
          	// Élément pas sélectionné, passe au suivant
          	si ZR_DossiersFichiers[i].ATT_Selection = faux ALORS
          		continuer
          	FIN
          	
          	
          	selon ZR_DossiersFichiers[i].ATT_Type
          	
          		// Si c'est un dossier
          		cas ZR_DossiersFichiers
          			SI zipAjouteRépertoire(sNomArchive,ZR_DossiersFichiers[i].ATT_CheminComplet) > 0 ALORS
          				Erreur(ErreurInfo(errComplet))
          				RETOUR
          			FIN
          			
          		// Si c'est un fichier
          		cas type_Fichier 
          			SI zipAjouteFichier(sNomArchive,ZR_DossiersFichiers[i].ATT_CheminComplet,zipAucun) > 0 ALORS
          				Erreur(ErreurInfo(errComplet))
          				RETOUR
          			FIN
          			
          			
          		AUTRE CAS
          			// Erreur 
          			erreur("Type de fichier inconnu")		
          	FIN
          FIN
          
          // Ferme l'archive
          zipFerme(sNomArchive)
          
          // Message à l'utilisateur
          ToastAffiche("Création de l'archive réussie")
          
          // Rafraîchit la fenêtre
          InitialisationFenêtre(gsRepEnCours+[fSep]+sNomArchiveComplet)
         type : 32
   -
